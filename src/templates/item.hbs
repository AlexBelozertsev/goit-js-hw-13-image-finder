{{#each this}}
<li>
    <div class="photo-card">
        <img src="{{webformatURL}}" data-source="{{largeImageURL}}" alt="author:{{user}}" />

        <div class="stats">
            <p class="stats-item">
                <i class="material-icons orange600">thumb_up</i>
                {{likes}}
            </p>
            <p class="stats-item">
                <i class="material-icons orange600">visibility</i>
                {{views}}
            </p>
            <p class="stats-item">
                <i class="material-icons orange600">comment</i>
                {{comments}}
            </p>
            <p class="stats-item">
                <i class="material-icons orange600">cloud_download</i>
                {{downloads}}
            </p>
        </div>
    </div>
</li>
{{/each}}